		{
			"name": "AccountSummaryReport",
			"item": [
				{
					"name": "Account Details",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var jsonData = pm.response.json().Data;\r",
									"\r",
									"pm.test(\"Validate Response Code\", function() {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"function returnNullIfAny(value) {\r",
									"    return value ? value.substring(0,10) : null;\r",
									"}\r",
									"\r",
									"function returnEmptyIfAny(value) {\r",
									"    return value ? value.substring(0,5) : '';\r",
									"}\r",
									"\r",
									"function returnDate(value) {\r",
									"    return value ? value.substring(0, 23) + \"Z\" : value;\r",
									"}\r",
									"\r",
									"function upperCaseFirstLetter(string){\r",
									"    return string.charAt(0).toUpperCase() + string.slice(1)\r",
									"}\r",
									"\r",
									"var validationTests = [\r",
									"    { name: 'reference', transform: val => val.toString() },\r",
									"    { name: 'ssn', transform: val => val.toString()},\r",
									"    { name: 'fullName' },\r",
									"    { name: 'firstName' },\r",
									"    { name: 'lastName' },\r",
									"    { name: 'gender', transform: returnNullIfAny},\r",
									"    { name: 'dateOfBirth', transform: returnNullIfAny },\r",
									"    { name: 'keyAccountManager', transform: returnEmptyIfAny },\r",
									"    { name: 'createdAt', transform: returnDate },\r",
									"    { name: 'updatedAt', transform: returnDate },\r",
									"    { name: 'type' },\r",
									"    { name: 'source' },\r",
									"    { name: 'status' },\r",
									"    { name: 'distributionMethod' },\r",
									"    { name: 'preferLanguage' },\r",
									"    { name: 'preferredCommunication' },\r",
									"    { name: 'country' },\r",
									"    { name: 'hasMarketingInfo', transform: val => val.toLowerCase() === 'true' }\r",
									"];\r",
									"\r",
									"validationTests.forEach(test => {\r",
									"    var jsonFieldName = test.name !== 'ssn' ? upperCaseFirstLetter(test.name) : test.name.toUpperCase();    \r",
									"    pm.test(`Validate ${jsonFieldName}`, function() {\r",
									"        var actualValue = jsonData[jsonFieldName];\r",
									"        var expectedValue = pm.variables.get(test.name);        \r",
									"        if (test.transform) {\r",
									"            expectedValue = test.transform(expectedValue);\r",
									"        }        \r",
									"        pm.expect(expectedValue).to.eql(actualValue);\r",
									"    });\r",
									"});\r",
									"\r",
									"function validateArrayCustomFields(variableName, fields) {\r",
									"    var jsonFieldName = upperCaseFirstLetter(variableName)\r",
									"    pm.test(`Validate ${jsonFieldName}`, function() {\r",
									"        var jsonDataArray = jsonData[jsonFieldName];\r",
									"        var variableData = JSON.parse(pm.variables.get(variableName));        \r",
									"        if (variableData === '{}') {\r",
									"            pm.expect(jsonDataArray).to.eql('{}');\r",
									"        } else {\r",
									"            for(let i=0; i<variableData.length;i++){\r",
									"                fields.forEach(field => {\r",
									"                    let expectedValue = variableData.field.name;\r",
									"                    if (field.transform) {\r",
									"                        expectedValue = field.transform(expectedValue);\r",
									"                    }\r",
									"                    pm.expect(expectedValue).to.eql(jsonDataArray[i][upperCaseFirstLetter(field.name)]);\r",
									"                });\r",
									"            }\r",
									"        }\r",
									"    });\r",
									"}\r",
									"\r",
									"validateArrayCustomFields('customFields', [\r",
									"    { name: 'PEP', transform: val => val === 'true' },\r",
									"    { name: 'deceased', transform: val => val === 'true' },\r",
									"    { name: 'nationality' },\r",
									"    { name: 'paymentOption' },\r",
									"    { name: 'citizenship'},\r",
									"    { name: 'personType' },\r",
									"    { name: 'companyType'}\r",
									"]);\r",
									"\r",
									"function validateArrayTest(variableName, fields) {\r",
									"    var jsonFieldName = upperCaseFirstLetter(variableName)\r",
									"    pm.test(`Validate ${jsonFieldName}`, function() {\r",
									"        const jsonDataArray = jsonData[jsonFieldName];\r",
									"        const variableData = JSON.parse(pm.variables.get(variableName));\r",
									"        \r",
									"        if (variableData === '[]') {\r",
									"            pm.expect(jsonDataArray).to.eql('[]');\r",
									"        } else {\r",
									"            variableData.forEach((item, i) => {\r",
									"                fields.forEach(field => {\r",
									"                    let expectValue = item[field.name];\r",
									"                    if (field.transform) {\r",
									"                        expectValue = field.transform(expectValue);\r",
									"                    }\r",
									"                    pm.expect(expectValue).to.eql(jsonDataArray[i][upperCaseFirstLetter(field.name)]);\r",
									"                });\r",
									"            });\r",
									"        }\r",
									"    });\r",
									"}\r",
									"\r",
									"validateArrayTest('phones', [\r",
									"    { name: 'number' },\r",
									"    { name: 'type'},\r",
									"    { name: 'createdAt', transform: returnDate },\r",
									"    { name: 'updatedAt', transform: returnDate }\r",
									"]);\r",
									"\r",
									"validateArrayTest('emails', [\r",
									"    { name: 'address' },\r",
									"    { name: 'type' },\r",
									"    { name: 'createdAt', transform: returnDate },\r",
									"    { name: 'updatedAt', transform: returnDate }\r",
									"]);\r",
									"\r",
									"validateArrayTest('addresses', [\r",
									"    { name: 'address' },\r",
									"    { name: 'city'},\r",
									"    { name: 'postCode' },\r",
									"    { name: 'type' },\r",
									"    { name: 'extraAddress'},\r",
									"    { name: 'country' },\r",
									"    { name: 'createdAt', transform: returnDate },\r",
									"    { name: 'updatedAt', transform: returnDate }\r",
									"]);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
